@Library('sharedlib1') _
pipeline{
    agent any
    stages{
        stage('sum of two numbers') {
            steps{
                script {
                    hello.sum(10 , 12)
                }
            }
        }
        stage('product of two numbers') {
            steps{
                script {
                    hello.product(12 , 2)
                }
            }
        }
    }
}
